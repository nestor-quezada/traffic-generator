# set terminal pngcairo  transparent enhanced font "arial,10" fontscale 1.0 size 600, 400 
# set output 'histograms.3.png'
set boxwidth 0.9 absolute
set style fill   solid 1.00 border lt -1
set key fixed right top vertical Right noreverse noenhanced autotitle nobox
set style increment default
set style histogram clustered gap 5 title textcolor lt -1
set datafile missing '-'
set style data histograms
#set xtics border in scale 0,0 nomirror rotate by -45  autojustify
set xtics  norangelimit 
set xtics   ()
set title "Probabilidades acumuladas tiempos entre llegadas (ms)" font ",25"
set xrange [ * : * ] noreverse writeback
set x2range [ * : * ] noreverse writeback
set yrange [ 0 : 1] noreverse writeback
set xlabel "Estados" font ",20"
set ylabel "Probabilidad acumulada P(X<x)" font ",25" offset -7,0
set tics font ",28"
set xtics offset 0,-1
set logscale y 10
set yrange [*:*]
set key font ",25" 
set key box width 1 height 3 opaque
set key outside
set lmargin at screen 0.08
set terminal png size 2600,600; set output 'casos_estados_tiemposcdf.png'; 
## Last datafile plotted: "immigration.dat"
plot 'casos_13e_20mins_xvar_times.txt' using 2:xtic(1) ti col, '' u 3 ti col, '' u 4 ti col, '' u 5 ti col,'' u 6 ti col

